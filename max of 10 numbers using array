#include<stdio.h>
int main()
{
    int arr[10],i,max=0;
    printf("\n enter 10 numbers \n\n");
    for(i=0;i<10;i++)
{
    printf("enter no %d \n",i+1);
     scanf("%d",&arr[i]);
}
    for(i=0;i<10;i++)
    {
        if(arr[i]>=max)
        {
        max=arr[i];
         }
    }
    printf("max is %d",max);
    return 0;
    
}
