#include<stdio.h>
int main()
{
    int arr[10],i,min;
    printf("\n enter 10 numbers \n\n");
    for(i=0;i<10;i++)
    {
        printf("enter number \n");
        scanf("%d",&arr[i]);
    }
    min=arr[0];
    for(i=0;i<10;i++)
    {
        if(arr[i]<=min){
       min=arr[i];
        }
    }
    printf("min is %d",min);
    return 0;
}
